<%= simple_form_for(@magasin) do |f| %>
  <%= f.error_notification %>
  
  <div class="form-inputs">
    <%= f.input :chain %>
    <%= f.input :name %>
    <%= f.input :longitude %>
    <%= f.input :latitude %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :zip %>
    <%= f.input :phone %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
